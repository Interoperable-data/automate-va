# 4. Issuing authority
eralex:PAVA-AnnexI-4 a skos:Concept , sh:NodeShape ;
    skos:inScheme eralex:AnnexIConceptScheme ;
    skos:prefLabel "(I) 4. Issuing authority"@en ;
    skos:definition "Issuing authority (Agency or national safety authority, as applicable)."@en ;
    skos:narrower eralex:PAVA-AnnexI-4-1, eralex:PAVA-AnnexI-4-2 ;
    sh:targetClass era:VehicleTypeAuthorisationCase ;
    sh:property [
        sh:path era:authorisingMemberState ;
        sh:minCount 1 ;
        sh:message "Issuing authority (era:authorisingMemberState) must be present for every VehicleTypeAuthorisationCase (Annex I, section 4)."@en ;
        sh:severity sh:Violation
    ] ;
    sh:or (
        [
            sh:path era:authorisingMemberState ;
            sh:hasValue <http://publications.europa.eu/resource/authority/country/EUR> ;
            sh:message "Annex I 4.1: The Agency is the issuing authority (era:authorisingMemberState = EUR)."@en ;
            sh:severity sh:Info
        ]
        [
            sh:path era:authorisingMemberState ;
            sh:in (
