# Harmonized and merged SKOS concepts for AnnexI-10-1 to AnnexI-10-8
# 10.1. Type identification
eralex:PAVA-AnnexI-10-1 a skos:Concept , sh:NodeShape ;
    skos:inScheme eralex:AnnexIConceptScheme ;
    skos:prefLabel "(I) 10.1. Type identification"@en ;
    skos:definition "Type identification (ID, name, date of record)."@en ;
    skos:broader eralex:PAVA-AnnexI-10 ;
    sh:targetClass era:VehicleTypeAuthorisationCase ;
    sh:sparql [
        sh:name "VehicleType identifier present (SPARQL)"@en ;
        sh:message "Each VehicleType of an AuthorisationCase must have a dcterms:identifier string (Annex I-10)."@en ;
        sh:severity sh:Violation ;
        sh:select '''
            PREFIX era: <http://data.europa.eu/949/>
            PREFIX vpa: <http://w3id.org/vpa#>
            PREFIX dcterms: <http://purl.org/dc/terms/>
            PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
            SELECT $this ?vehicletype ?identifier
            WHERE {
              $this vpa:concerns ?vehicletype .
              ?vehicletype a era:VehicleType .
              FILTER NOT EXISTS {
                ?vehicletype dcterms:identifier ?identifier .
                FILTER (datatype(?identifier) = xsd:string)
              }
            }
        '''
    ] ;

    dcterms:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX%3A02018R0545-20200616#anx_I> .

# 10.2. Vehicle type versions (when applicable)
eralex:PAVA-AnnexI-10-2 a skos:Concept , sh:NodeShape ;
    skos:inScheme eralex:AnnexIConceptScheme ;
    skos:prefLabel "(I) 10.2. Vehicle type versions (when applicable)"@en ;
    skos:definition "Vehicle type versions as defined in Decision 2011/665/EU Annex II, when applicable."@en ;
    skos:broader eralex:PAVA-AnnexI-10 ;
    sh:targetClass era:VehicleTypeAuthorisationCase ;
    sh:sparql [
        sh:name "VehicleType is before 4th RP (era:includedVersions present)"@en ;
        sh:message "(I) 10.2. VehicleType is before the 4th Railway Package: era:includedVersions is present. dcterms:identifier and era:vehicleTypeNumber must also be present."@en ;
        sh:severity sh:Warning ;
        sh:select '''
            PREFIX era: <http://data.europa.eu/949/>
            PREFIX vpa: <http://w3id.org/vpa#>
            PREFIX dcterms: <http://purl.org/dc/terms/>
            SELECT $this ?vehicletype
            WHERE {
              $this vpa:concerns ?vehicletype .
              ?vehicletype era:includedVersions ?v .
              FILTER NOT EXISTS { ?vehicletype dcterms:identifier ?id }
              FILTER NOT EXISTS { ?vehicletype era:vehicleTypeNumber ?num }
            }
        '''
    ] ;
    sh:sparql [
        sh:name "VehicleType cannot have both includedVersions and includedVersionsVariant"@en ;
        sh:message "(I) 10.2. VehicleType cannot have both era:includedVersions and era:includedVersionsVariant present at the same time."@en ;
        sh:severity sh:Violation ;
        sh:select '''
            PREFIX era: <http://data.europa.eu/949/>
            PREFIX vpa: <http://w3id.org/vpa#>
            SELECT $this ?vehicletype
            WHERE {
              $this vpa:concerns ?vehicletype .
              ?vehicletype era:includedVersions ?v1 .
              ?vehicletype era:includedVersionsVariant ?v2 .
            }
        '''
    ] ;
    dcterms:source <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX%3A02018R0545-20200616#anx_I> .

