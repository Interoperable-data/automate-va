    vpa:checkedSection era-lex-sections:TSI-WAG-Section-6.2.2.1.StrengthOfUnit ; # See also the Non-disclosed properties
    vpa:isCompliant "false"^^xsd:boolean ;
    vpa:withRestriction [                                     # subProperty of vpa:withRestriction
        a era:CABRestriction ;                        # subClassOf vpa:Restriction ;
        vpa:regarding eralex:reg-2015-924 ;
        dcterms:source eralex:reg-2015-924 ;
        rdfs:comment "Example: Original text of the NoBo's restriction can be provided here" ;
    ].

