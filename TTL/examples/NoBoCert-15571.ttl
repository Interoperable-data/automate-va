era-cld:cld-NoBoCert-15571 
    a era:CLD , vpa:EvidenceDocument ; 
    dcterms:created "2024-12-16"^^xsd:date ; # the creation of this resource in the KG
    dcterms:issued "2021-09-23"^^xsd:date ; # the document issue date
    dcterms:replaces era-cld:cld-NoBoCert-7370 ;

    # What this CLD uses as legal basis:
    dcterms:type era-cld-types:1 ;
    dcterms:conformsTo era-cld-modules:SB ; # the module
    dcterms:source eralex:dir-2016-797 ; # the EU Legislation
    dcterms:subject eralex:reg-2015-924 ; # The RST subsystem as a whole
    vpa:checkedCompliance [ 
        vpa:checkedRequirement 
            eralex:reg-2013-1236 , 
            eralex:reg-2014-1304 ,
            eralex:reg-2015-924 ,
            eralex:reg-2013-321 
    ] ;

    # Human readable content
    dcterms:description "Tank Wagon 97m3 Za(c)ns" ;
    dcterms:identifier "2826/1/SB/2021/RST/EN/N20210917/01" ;
    rdfs:seeAlso "https://eradis.era.europa.eu/interop_docs/NoBoCert/view.aspx?id=15571"^^xsd:anyURI ;
    vpa:valid [
        a time:ProperInterval ;
        time:hasBeginning "2016-09-02"^^xsd:date ; #start of validity
        time:hasDurationDescription [
            # https://www.w3.org/TR/owl-time/#time:DurationDescription
            time:years 10 ;
        ]
    ];
    rdfs:comment """This certificate follows certificate number 0038/1/SB/2016/RST/EN/20161113 issue 2 dated
02/09/2016 and certificate number 2673/1/SB/2016/RST/EN/N20160045/02 dated 29/05/2020"""@en ;
    dcterms:audience [ a era:OrganisationRole ; era:roleOf rorg:MF-RO-310141-2931 ; era:hasOrganisationRole era-organisation-roles:Applicant  ] ; # Applicant 
    dcterms:contributor [ a era:OrganisationRole ; era:roleOf rorg:MF-RO-310141-2931 ; era:hasOrganisationRole era-organisation-roles:Manufacturer ] ; # Manufacturer
    dcterms:creator [ a era:OrganisationRole ; era:roleOf rorg:NoBo-2826 ; era:hasOrganisationRole era-organisation-roles:NoBo ] ; # The NoBo itself /ORGS (<http://data.europa.eu/949/taxonomies/org#NoBo-2826>)
    # dcterms:spatial [ a org:Site ] ; # No manufacturing locations
    vpa:checkedCompliance
        erapod:check-NoBoCert-15571-1 ,
        erapod:check-NoBoCert-15571-2 ;
    vpa:versionNumber 1 .

# Stored in a confidential data pod and inaccessible
